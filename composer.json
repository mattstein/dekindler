{
    "name": "mattstein/kindle-clipping-extractor",
    "description": "Extract Kindle highlights, notes, and bookmarks.",
    "license": "MIT",
    "authors": [
        {
            "name": "Matt Stein",
            "email": "m@ttste.in"
        }
    ],
    "autoload": {
        "psr-4": {
            "mattstein\\utilities\\": "src/"
        }
    },
    "require": {
        "php": ">=8.1",
        "symfony/console": "^6.2.0",
        "ext-iconv": "*"
    },
    "require-dev": {
        "pestphp/pest": "^1.22"
    },
    "scripts": {
        "test": "./vendor/bin/pest",
        "demo": "php -S localhost:8000 -t demo/"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
